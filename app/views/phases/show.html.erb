<% content_for :previous_url do %>
  <%= previous_program_navigation_path({:phase_id => @phase.to_param}) %>
<% end %>
<% content_for :next_url do %>
  <%= next_program_navigation_path({:phase_id => @phase.to_param}) %>
<% end %>

<div class="current-step phase">
  <div class="heading">
    <h1><%=@phase.headline%></h1>
    <h2><%=@phase.description%></h2>
  </div>
</div>
